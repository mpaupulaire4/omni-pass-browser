{
  "name": "OmniPass Browser Extension",
  "version": "1.0",
  "description": "A browser extension implementation of OmniPass",
  "permissions": ["storage", "tabs"],
  "browser_action": {
    "default_title": "OmniPass",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}